{
    "swagger": "2.0",
    "info": {
        "description": "API documentation for IEIProject",
        "version": "1.0.0",
        "title": "IEIProject API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "you@example.com"
        }
    },
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [
        {
            "name": "Extractors",
            "description": "Endpoints related to data extraction"
        },
        {
            "name": "Load",
            "description": "Endpoints related to loading data"
        },
        {
            "name": "Utility",
            "description": "Utility endpoints"
        }
    ],
    "paths": {
        "/extractor/execute_csv": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute CSV extraction",
                "description": "Endpoint to execute CSV extraction.\n\n**Postman Instructions:**\n- **Method:** POST\n- **URL:** http://localhost:8080/extractor/execute_csv\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/extractor/execute_json": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute JSON extraction",
                "description": "Endpoint to execute JSON extraction.\n\n**Postman Instructions:**\n- **Method:** POST\n- **URL:** http://localhost:8080/extractor/execute_json\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/extractor/execute_xml": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute XML extraction",
                "description": "Endpoint to execute XML extraction.\n\n**Postman Instructions:**\n- **Method:** POST\n- **URL:** http://localhost:8080/extractor/execute_xml\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/load_results": {
            "get": {
                "tags": [
                    "Load"
                ],
                "summary": "Get log summary",
                "description": "Fetches the log summary from the log file.\n\n**Postman Instructions:**\n- **Method:** GET\n- **URL:** http://localhost:8080/load_results\n- **Headers:** None\n- **Body:** None",
                "produces": [
                    "text/plain"
                ],
                "responses": {
                    "200": {
                        "description": "Log summary fetched successfully",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "Log file not found",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Log file not found"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/clear_files": {
            "post": {
                "tags": [
                    "Utility"
                ],
                "summary": "Clear necessary JSON and log files",
                "description": "Endpoint to clear necessary JSON and log files.\n\n**Postman Instructions:**\n- **Method:** POST\n- **URL:** http://localhost:8080/clear_files\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Files cleared successfully",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "Files cleared successfully"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Internal server error"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/total_correct_loads": {
            "get": {
                "tags": [
                    "Utility"
                ],
                "summary": "Get total correct loads",
                "description": "Fetches the total number of correct loads.\n\n**Postman Instructions:**\n- **Method:** GET\n- **URL:** http://localhost:8080/total_correct_loads\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Total correct loads fetched successfully",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "total": {
                                    "type": "integer",
                                    "example": 42
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Internal server error"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}