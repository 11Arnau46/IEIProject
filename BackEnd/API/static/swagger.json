{
    "swagger": "2.0",
    "info": {
        "description": "API documentation for IEIProject",
        "version": "1.0.0",
        "title": "IEIProject API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "you@example.com"
        }
    },
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [
        {
            "name": "Extractors",
            "description": "Endpoints related to data extraction"
        },
        {
            "name": "Load",
            "description": "Endpoints related to loading data"
        }
    ],
    "paths": {
        "/extractor/execute_csv": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute CSV extraction",
                "description": "Endpoint to execute CSV extraction",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/extractor/execute_json": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute JSON extraction",
                "description": "Endpoint to execute JSON extraction",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/extractor/execute_xml": {
            "post": {
                "tags": [
                    "Extractors"
                ],
                "summary": "Execute XML extraction",
                "description": "Endpoint to execute XML extraction",
                "responses": {
                    "200": {
                        "description": "Extraction successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/load_results": {
            "get": {
                "tags": [
                    "Load"
                ],
                "summary": "Get log summary",
                "description": "Fetches the log summary from the log file",
                "produces": [
                    "text/plain"
                ],
                "responses": {
                    "200": {
                        "description": "Log summary fetched successfully",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string",
                                    "example": "Log entry 1\nLog entry 2\nLog entry 3"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Log file not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Log file not found"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}