{
    "swagger": "2.0",
    "info": {
        "description": "API para la transformación de datos de CSV a JSON",
        "version": "1.0.0",
        "title": "WrapperCSV API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "agarmar6@etsinf.upv.es"
        }
    },
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [
        {
            "name": "WrapperCSV",
            "description": "Endpoints para la transformación de datos de CSV a JSON"
        },
        {
            "name": "WrapperCSV Logs",
            "description": "Endpoints para la gestión de logs"
        }
    ],
    "paths": {
        "/wrapperCSV/execute": {
            "post": {
                "tags": [
                    "WrapperCSV"
                ],
                "summary": "Ejecutar transformación de CSV a JSON",
                "description": "Endpoint para ejecutar la transformación de datos de CSV a JSON.",
                "responses": {
                    "200": {
                        "description": "Transformación de datos exitosa. Los resultados obtenidos son:",
                        "examples": {
                            "application/json": [
                                {
                                    "nomMonumento": "",
                                    "tipoMonumento": "",
                                    "direccion": "",
                                    "codigo_postal": "",
                                    "longitud": "",
                                    "latitud": "",
                                    "descripcion": "",
                                    "nomLocalidad": "",
                                    "nomProvincia": ""
                                }
                            ]
                        }
                    },
                    "404": {
                        "description": "Archivo de salida no encontrado",
                        "examples": {
                            "application/json": {
                                "error": "Output file not found"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Subprocess failed with error: <error_message>"
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "WrapperCSV"
                ],
                "summary": "Eliminar archivo JSON generado",
                "description": "Endpoint para eliminar el archivo JSON generado por la transformación.",
                "responses": {
                    "200": {
                        "description": "Archivo eliminado exitosamente",
                        "examples": {
                            "application/json": {
                                "message": "Output file deleted successfully"
                            }
                        }
                    },
                    "404": {
                        "description": "Archivo no encontrado",
                        "examples": {
                            "application/json": {
                                "error": "Output file not found"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "An error occurred while deleting the output file: <error_message>"
                            }
                        }
                    }
                }
            }
        },
        "/wrapperCSV/log/estadisticas": {
            "get": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Obtener log de estadísticas",
                "description": "Endpoint para obtener el contenido del archivo de log de estadísticas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log obtenido exitosamente",
                        "schema": {
                            "type": "string"
                        },
                        "examples": {
                            "text/plain": "Número de registros cargados correctamente : NN\n"
                        }
                    },
                    "404": {
                        "description": "Archivo de log no encontrado",
                        "examples": {
                            "application/json": {
                                "error": "Log de estadísticas no encontrado"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al leer el log de estadísticas: <error_message>"
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Limpiar log de estadísticas",
                "description": "Endpoint para limpiar el contenido del archivo de log de estadísticas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log limpiado exitosamente",
                        "examples": {
                            "application/json": {
                                "message": "Log de estadísticas limpiado exitosamente"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al limpiar el log de estadísticas: <error_message>"
                            }
                        }
                    }
                }
            }
        },
        "/wrapperCSV/log/rechazados": {
            "get": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Obtener log de rechazados",
                "description": "Endpoint para obtener el contenido del archivo de log de entradas rechazadas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log obtenido exitosamente",
                        "schema": {
                            "type": "string"
                        },
                        "examples": {
                            "text/plain": "Registros con errores y descartados: {fuente, nombre, Localidad, motivo del error}\n"
                        }
                    },
                    "404": {
                        "description": "Archivo de log no encontrado",
                        "examples": {
                            "application/json": {
                                "error": "Log de rechazados no encontrado"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al leer el log de rechazados: <error_message>"
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Limpiar log de rechazados",
                "description": "Endpoint para limpiar el contenido del archivo de log de entradas rechazadas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log limpiado exitosamente",
                        "examples": {
                            "application/json": {
                                "message": "Log de rechazados limpiado exitosamente"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al limpiar el log de rechazados: <error_message>"
                            }
                        }
                    }
                }
            }
        },
        "/wrapperCSV/log/reparados": {
            "get": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Obtener log de reparados",
                "description": "Endpoint para obtener el contenido del archivo de log de entradas reparadas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log obtenido exitosamente",
                        "schema": {
                            "type": "string"
                        },
                        "examples": {
                            "text/plain": "Registros con errores y reparados: {fuente, nombre, Localidad, motivo del error, operación realizada}\n"
                        }
                    },
                    "404": {
                        "description": "Archivo de log no encontrado",
                        "examples": {
                            "application/json": {
                                "error": "Log de reparados no encontrado"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al leer el log de reparados: <error_message>"
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Limpiar log de reparados",
                "description": "Endpoint para limpiar el contenido del archivo de log de entradas reparadas.",
                "responses": {
                    "200": {
                        "description": "Contenido del log limpiado exitosamente",
                        "examples": {
                            "application/json": {
                                "message": "Log de reparados limpiado exitosamente"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al limpiar el log de reparados: <error_message>"
                            }
                        }
                    }
                }
            }
        },
        "/wrapperCSV/log": {
            "delete": {
                "tags": [
                    "WrapperCSV Logs"
                ],
                "summary": "Limpiar todos los logs",
                "description": "Endpoint para limpiar el contenido de todos los archivos de log.",
                "responses": {
                    "200": {
                        "description": "Contenido de todos los logs limpiado exitosamente",
                        "examples": {
                            "application/json": {
                                "message": "Todos los archivos de log han sido limpiados exitosamente"
                            }
                        }
                    },
                    "500": {
                        "description": "Error interno del servidor",
                        "examples": {
                            "application/json": {
                                "error": "Error al limpiar los archivos de log: <error_message>"
                            }
                        }
                    }
                }
            }
        }
    }
}