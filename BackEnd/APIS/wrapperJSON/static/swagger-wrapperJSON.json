                "responses": {
                    "200": {
                        "description": "Result fetched successfully",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "404": {
                        "description": "Result not found",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Result not found"
                                }
                            }
                        }
                    }
                }
            }
        },
