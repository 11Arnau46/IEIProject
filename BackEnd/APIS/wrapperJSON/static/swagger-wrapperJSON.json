    "paths": {
        "/wrapperJSON/execute": {
            "post": {
                "tags": [
                    "WrapperJSON"
                ],
                "summary": "Execute data transformation to JSON",
                "description": "Endpoint to execute the transformation of the data source to the desired format (JSON).\n\n**Postman Instructions:**\n- **Method:** POST\n- **URL:** http://localhost:8082/wrapperJSON/execute\n- **Headers:** None\n- **Body:** None",
                "responses": {
                    "200": {
                        "description": "Transformation successful"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
