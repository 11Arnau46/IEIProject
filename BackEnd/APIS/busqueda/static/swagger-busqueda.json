{
    "swagger": "2.0",
    "info": {
        "description": "API documentation for Busqueda",
        "version": "1.0.0",
        "title": "Busqueda API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "you@example.com"
        }
    },
    "host": "localhost:5001",
    "basePath": "/",
    "tags": [
        {
            "name": "Busqueda",
            "description": "Endpoints related to data search"
        }
    ],
    "paths": {
        "/api/monumentos": {
            "get": {
                "tags": [
                    "Busqueda"
                ],
                "summary": "Get all monuments",
                "description": "Endpoint to get all monuments with optional query parameters for filtering.\n\n**Postman Instructions:**\n- **Method:** GET\n- **URL:** http://localhost:5001/api/monumentos\n- **Headers:** None\n- **Body:** None",
                "parameters": [
                    {
                        "name": "localidad",
                        "in": "query",
                        "description": "Filter by locality",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "codigo_postal",
                        "in": "query",
                        "description": "Filter by postal code",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "provincia",
                        "in": "query",
                        "description": "Filter by province",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "tipo",
                        "in": "query",
                        "description": "Filter by type of monument",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Monuments fetched successfully",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object"
                            }
                        }
                    },
                    "404": {
                        "description": "Monuments not found",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string",
                                    "example": "Monuments not found"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}